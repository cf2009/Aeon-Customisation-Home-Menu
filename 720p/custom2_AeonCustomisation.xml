				</control>

				<control type="image" id="88">
					<visible>Control.IsVisible(300) | Control.IsVisible(301)</visible>
					<animation effect="fade" time="300">Hidden</animation>
				</control>

				<control type="multiimage">
					<include>BackgroundVarsVirtual</include>
